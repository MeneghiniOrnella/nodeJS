    <main>
        <div>
            <% for (let product of prods) { %>
            <article>
                <h1 class="product__title"><%= product.title %></h1>
                <img src="<product.imageUrl %>" alt="<%= product.imageUrl %>">
                <h4><%= product.price %></h4>
                <p><%= product.description %></p>
                <button type="submit">Add product</button>
            </article>
            <% } %>
        </div>
    </main>
