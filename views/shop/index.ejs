    <main>
        <% if (prods.length >0) { %>
            <div>
                <% for (let product of prods) %>
                    <article>
                        <h2><%- product.title %></h2>
                        <!-- <img src="product.imageUrl" alt="product.imageUrl"> -->
                        <h4>product.price</h4>
                        <p>product.description</p>
                        <button type="submit">Add product</button>
                    </article>
                <% }else{ %>
                    <p>nothing</p>
                <% } %>
            </div>
        <% } %>
    </main>
